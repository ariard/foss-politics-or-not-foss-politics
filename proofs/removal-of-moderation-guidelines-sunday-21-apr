    <div>
      

      <strong>
            <a class="author Link--primary text-bold css-overflow-wrap-anywhere " show_full_name="false" data-hovercard-type="user" data-hovercard-url="/users/laanwj/hovercard" data-octo-click="hovercard-link-click" data-octo-dimensions="link_type:self" href="/laanwj">laanwj</a>
  

